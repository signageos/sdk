{
  "name": "@signageos/sdk",
  "version": "1.27.0",
  "main": "./dist",
  "files": [
    "dist",
    ".env",
    "README.md",
    "package.json"
  ],
  "repository": {
    "url": "https://github.com/signageos/sdk",
    "type": "git"
  },
  "scripts": {
    "build": "npx check-engine@latest && tsc",
    "lint": "eslint --ext .ts,.tsx,.js ./src/ ./tests",
    "clean": "rm -rf dist/*",
    "clean-build": "npm run clean && npm run build && git config blame.ignoreRevsFile .git-blame-ignore-revs",
    "test": "env NODE_ENV=test mocha",
    "test:unit": "env NODE_ENV=test mocha --config .mocharc.unit.json",
    "test:coverage": "c8 npm run test",
    "ci": "npm run lint && npm run test",
    "watch": "tsc --watch",
    "docs": "typedoc ./src/index.ts ./src/RestApi/RestApi.ts ./src/RestApi/RestApiV2.ts ./src/Development/Development.ts",
    "check": "npm run depcheck && npx --userconfig ./.npmrc @signageos/lib check-deps",
    "depcheck": "depcheck --config .depcheckrc.json",
    "check-types": "tsc --noEmit"
  },
  "author": "Michael Zabka <zabka.michael@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "engineStrict": false,
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@signageos/codestyle": "0.2.0",
    "@types/archiver": "5.3.1",
    "@types/chalk": "2.2.0",
    "@types/cors": "2.8.13",
    "@types/debug": "4.1.7",
    "@types/express": "4.17.17",
    "@types/faker": "4.1.5",
    "@types/form-data": "2.5.0",
    "@types/jsdom": "16.1.0",
    "@types/lodash": "4.14.116",
    "@types/mocha": "10.0.1",
    "@types/moment-timezone": "0.5.4",
    "@types/nock": "10.0.3",
    "@types/node": "20.12.7",
    "@types/node-fetch": "2.6.9",
    "@types/should": "8.3.0",
    "@types/sinon": "9.0.8",
    "c8": "7.13.0",
    "depcheck": "1.4.3",
    "extract-zip": "2.0.1",
    "faker": "4.1.0",
    "lodash": "4.17.21",
    "mocha": "10.2.0",
    "nock": "13.5.4",
    "should": "11.2.1",
    "sinon": "9.2.0",
    "stream-to-string": "1.2.0",
    "ts-node": "10.9.1",
    "typedoc": "0.23.23",
    "typedoc-plugin-missing-exports": "1.0.0",
    "typedoc-plugin-rename-defaults": "0.6.4",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "@signageos/forward-server-bridge": "0.0.4",
    "@signageos/front-applet": "7.0.0",
    "@types/fs-extra": "8.1.0",
    "@types/geojson": "7946.0.8",
    "@types/ini": "1.3.30",
    "archiver": "5.3.1",
    "body-parser": "1.20.3",
    "chalk": "2.4.2",
    "chokidar": "3.5.3",
    "cors": "2.8.5",
    "debug": "4.3.4",
    "dotenv": "16.4.1",
    "express": "4.18.2",
    "file-type": "16.5.3",
    "form-data": "4.0.0",
    "fs-extra": "8.1.0",
    "geojson": "0.5.0",
    "globby": "11.1.0",
    "ini": "1.3.6",
    "jsdom": "18.0.1",
    "node-fetch": "2.7.0"
  },
  "description": "Library which allows you to fully manage signageOS applets, devices, management & monitoring using JS."
}
