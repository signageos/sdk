{
  "name": "@signageos/sdk",
  "version": "1.9.0",
  "main": "./dist",
  "files": [
    "dist",
    "config",
    ".env",
    "README.md",
    "package.json",
    "package-lock.json"
  ],
  "repository": {
    "url": "https://github.com/signageos/sdk",
    "type": "git"
  },
  "scripts": {
    "build": "npx check-engine && tsc",
    "lint": "tslint --config node_modules/@signageos/codestyle/tslint.json {src,tests}/**/*.{ts,tsx}",
    "prebuild": "rm -rf dist/*",
    "prepare": "npm run prebuild && npm run build",
    "test": "env NODE_ENV=test mocha --opts mocha.opts",
    "ci": "npm run lint && npm run test",
    "watch": "tsc --watch",
    "docs": "typedoc ./src/RestApi",
    "check": "npm run depcheck && npx --userconfig ./.npmrc @signageos/lib check-deps",
    "depcheck": "depcheck --specials=tslint,webpack,mocha --parsers='*.ts:typescript,*.tsx:typescript,*.js:es6' --detectors='requireCallExpression,importDeclaration' --ignore-dirs='dist' --ignores='@types/*,@signageos/codestyle,ts-node,source-map-support,mocha,depcheck,webpack-cli,typedoc,prettier'"
  },
  "author": "Michael Zabka <zabka.michael@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "engineStrict": false,
  "publishConfig": {
    "registry": "https://npm.signageos.io",
    "access": "public"
  },
  "devDependencies": {
    "@signageos/codestyle": "0.0.21",
    "@types/faker": "4.1.5",
    "@types/form-data": "2.5.0",
    "@types/jsdom": "16.1.0",
    "@types/lodash": "4.14.116",
    "@types/mocha": "7.0.1",
    "@types/moment-timezone": "0.5.4",
    "@types/nock": "10.0.3",
    "@types/node": "12.12.29",
    "@types/node-fetch": "2.1.2",
    "@types/should": "8.3.0",
    "@types/sinon": "9.0.8",
    "@types/webpack": "3.8.1",
    "depcheck": "1.2.0",
    "faker": "4.1.0",
    "mocha": "7.2.0",
    "nock": "10.0.6",
    "prettier": "2.5.1",
    "should": "11.2.1",
    "sinon": "9.2.0",
    "stream-to-string": "1.2.0",
    "ts-node": "8.6.2",
    "tslint": "6.1.3",
    "typedoc": "0.19.2",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@signageos/front-applet": "5.2.0",
    "@signageos/lib": "9.2.0",
    "@types/fs-extra": "8.1.0",
    "@types/geojson": "7946.0.8",
    "@types/ini": "1.3.30",
    "dotenv": "8.0.0",
    "form-data": "2.5.1",
    "fs-extra": "8.1.0",
    "geojson": "0.5.0",
    "ini": "1.3.6",
    "jsdom": "18.0.1",
    "lodash": "4.17.21",
    "node-fetch": "2.6.7"
  },
  "description": "Library which allows you to fully manage signageOS applets, devices, management & monitoring using JS."
}
