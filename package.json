{
  "name": "@signageos/sdk",
  "version": "1.29.0",
  "main": "./dist",
  "files": [
    "dist",
    ".env",
    "README.md",
    "package.json"
  ],
  "repository": {
    "url": "https://github.com/signageos/sdk",
    "type": "git"
  },
  "scripts": {
    "clean": "rm -rf dist/*",
    "build": "tsc",
    "clean-build": "npm run clean && npm run build",
    "watch": "tsc --watch",
    "check-types": "tsc --noEmit && tsc --project tsconfig.test.json --noEmit",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:prettier": "prettier  \"(src|tests|tools)/**/*.+(ts|tsx|json|js)\" --check",
    "lint:prettier:fix": "prettier  \"(src|tests|tools)/**/*.+(ts|tsx|json|js)\" --write",    
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "env NODE_ENV=test mocha --config .mocharc.unit.json",
    "test:integration": "env NODE_ENV=test mocha --config .mocharc.integration.json",
    "test:e2e": "env NODE_ENV=test mocha --config .mocharc.e2e.json",
    "test:coverage": "c8 npm test",
    "ci": "npm run lint && npm test",
    "check": "npm run depcheck && npx --userconfig ./.npmrc @signageos/lib-ci check-deps",
    "depcheck": "depcheck --config .depcheckrc.json",
    "docs": "typedoc ./src/index.ts ./src/RestApi/RestApi.ts ./src/RestApi/RestApiV2.ts ./src/Development/Development.ts"
  },
  "author": "Michael Zabka <zabka.michael@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "engineStrict": false,
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@signageos/codestyle": "0.4.0",
    "@swc/core": "1.11.18",
    "@types/archiver": "5.3.4",
    "@types/cors": "2.8.19",
    "@types/debug": "4.1.7",
    "@types/express": "4.17.23",
    "@types/faker": "4.1.12",
    "@types/jsdom": "21.1.7",
    "@types/lodash": "4.17.20",
    "@types/mocha": "10.0.10",
    "@types/moment-timezone": "0.5.30",
    "@types/nock": "10.0.3",
    "@types/node": "20.12.7",
    "@types/node-fetch": "2.6.13",
    "@types/should": "8.3.0",
    "@types/sinon": "9.0.11",
    "c8": "7.13.0",
    "depcheck": "1.4.7",
    "extract-zip": "2.0.1",
    "faker": "4.1.0",
    "lodash": "4.17.21",
    "mocha": "10.8.2",
    "nock": "13.5.6",
    "should": "11.2.1",
    "sinon": "9.2.4",
    "stream-to-string": "1.2.1",
    "ts-node": "10.9.2",
    "typedoc": "0.28.12",
    "typedoc-plugin-missing-exports": "4.0.0",
    "typedoc-plugin-rename-defaults": "0.7.3",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@signageos/forward-server-bridge": "0.0.6",
    "@signageos/front-applet": "8.1.0",
    "@types/fs-extra": "8.1.5",
    "@types/geojson": "7946.0.16",
    "@types/ini": "1.3.34",
    "archiver": "5.3.2",
    "body-parser": "2.2.0",
    "chalk": "2.4.2",
    "chokidar": "3.6.0",
    "cors": "2.8.5",
    "debug": "4.3.4",
    "dotenv": "16.4.1",
    "express": "5.1.0",
    "file-type": "21.0.0",
    "form-data": "4.0.4",
    "fs-extra": "8.1.0",
    "geojson": "0.5.0",
    "globby": "11.1.0",
    "ini": "1.3.8",
    "jsdom": "26.1.0",
    "node-fetch": "2.7.0",
    "zod": "3.24.2"
  },
  "description": "Library which allows you to fully manage signageOS applets, devices, management & monitoring using JS."
}
